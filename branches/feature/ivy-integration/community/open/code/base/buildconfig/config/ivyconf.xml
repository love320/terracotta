<ivyconf>
  <conf defaultResolver="ibiblio"/>
  <resolvers>
    <url name="tc-repo">
      <artifact pattern="http://eellispc.terracotta.lan/~eellis/lib/[artifact]-[revision].[ext]"/>
    </url>
    <ibiblio name="ibiblio"/>
  </resolvers>
  <modules>
	<module organisation="terracotta" name=".*" resolver="tc-repo"/>
  </modules>
</ivyconf>
