<?xml version="1.0"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.terracotta.plugins</groupId>
  <artifactId>awt-core</artifactId>
  <name>awt-core</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://www.terracotta.org</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <index>false</index>
            <manifest/>
            <manifestEntries>
              <Bundle-Activator>org.terracotta.plugins.instrumentation.awt.AWTTerracottaInstrumentation</Bundle-Activator>
              <Bundle-SymbolicName>${pom.groupId}.${pom.artifactId}</Bundle-SymbolicName>
              <Bundle-Version>${pom.version}</Bundle-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.osgi</groupId>
      <artifactId>osgi_R4_core</artifactId>
      <version>1.0</version>
    </dependency>
  </dependencies>
</project>